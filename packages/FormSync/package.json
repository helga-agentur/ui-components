{
  "name": "@joinbox/formsync",
  "version": "2.2.1",
  "description": "Synchronizes forms and elements between an original form (e.g. one created by Drupal) and another form (e.g. freely created as a template based on mockups).",
  "main": "FormSyncElement.js",
  "scripts": {
    "test": "npx ava --verbose",
    "build": "npx rollup --c rollup.config.js"
  },
  "author": "Felix Steiner <felix@joinbox.com>",
  "license": "ISC",
  "ava": {
    "files": [
      "**/*.unit.mjs",
      "**/*.integration.mjs"
    ],
    "nodeArguments": [
      "--experimental-modules"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@joinbox/eslint-config-joinbox": "^1.0.10",
    "ava": "^4.2.0",
    "jsdom": "^19.0.0",
    "lerna": "^4.0.0",
    "rollup": "^2.70.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
