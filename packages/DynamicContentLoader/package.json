{
  "name": "@helga-agentur/dynamic-content-loader",
  "version": "1.0.0",
  "description": "Extensible, loosely coupled custom elements that load content asynchronously, e.g. for filtered search results",
  "main": "main.js",
  "scripts": {
    "test": "npx ava --verbose",
    "build": "npx rollup --c rollup.config.mjs"
  },
  "author": "Felix Steiner <felix@helga.ch>",
  "license": "ISC",
  "ava": {
    "files": [
      "**/*.unit.mjs",
      "**/*.integration.mjs"
    ],
    "nodeArguments": [
      "--experimental-modules"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@joinbox/eslint-config-joinbox": "^1.0.11",
    "@rollup/plugin-terser": "^0.4.4",
    "ava": "^5.3.1",
    "jsdom": "^22.1.0",
    "lerna": "^7.1.0",
    "rollup": "^3.25.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
